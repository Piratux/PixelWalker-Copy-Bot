{
  "name": "pixelwalker-copy-bot",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "format": "npx prettier . --write --ignore-pattern dist",
    "lint": "npx eslint . --fix --ignore-pattern dist",
    "ts-check": "vue-tsc --noEmit",
    "gen-files": "npx tsx src/script/GenerateBlockEnumFiles.ts",
    "check-dependencies": "npx npm-check-updates",
    "update-dependencies": "npx npm-check-updates -u && npm install"
  },
  "dependencies": {
    "@basementuniverse/vec": "^2.2.2",
    "@mdi/font": "^7.4.47",
    "@tonejs/midi": "^2.0.28",
    "async-wait-until": "^2.0.27",
    "iconv-lite": "^0.6.3",
    "izitoast": "^1.4.0",
    "lodash-es": "^4.17.21",
    "many-keys-map": "^2.0.1",
    "pinia": "^3.0.1",
    "playerioclient": "^1.0.6",
    "pngjs": "^7.0.0",
    "pw-js-api": "^0.3.10",
    "pw-js-world": "^0.4.1-dev.766d8c6",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0",
    "vuetify": "^3.8.1",
    "worker-timers": "^8.0.20"
  },
  "engines": {
    "node": ">=22.12.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.24.0",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^22.14.0",
    "@types/pngjs": "^6.0.5",
    "@vitejs/plugin-vue": "^5.2.3",
    "@vue/eslint-config-typescript": "^14.5.0",
    "eslint": "^9.24.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-vue": "^10.0.0",
    "globals": "^16.0.0",
    "lint-staged": "^16.1.2",
    "prettier": "^3.6.2",
    "simple-git-hooks": "^2.13.0",
    "tsx": "^4.19.3",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.29.1",
    "vite": "^6.2.5",
    "vite-plugin-checker": "^0.9.1",
    "vite-plugin-eslint2": "^5.0.3",
    "vite-plugin-node-polyfills": "^0.23.0",
    "vite-tsconfig-paths": "^5.1.4",
    "vue-tsc": "^2.2.8"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged && npm run ts-check"
  },
  "lint-staged": {
    "*": [
      "prettier --write --ignore-unknown",
      "eslint --fix"
    ]
  }
}

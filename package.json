{
  "name": "pixelwalker-copy-bot",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "format": "npx prettier . --write --ignore-pattern dist",
    "lint": "npx eslint . --fix --ignore-pattern dist",
    "ts-check": "vue-tsc --noEmit",
    "check-dependencies": "npx npm-check-updates",
    "update-dependencies": "npx npm-check-updates -u && npm install",
    "gen-files": "npx tsx src/script/GenerateBlockEnumFiles.ts",
    "gen-tests": "npx tsx src/script/GenerateTests.ts"
  },
  "dependencies": {
    "@basementuniverse/vec": "^2.4.0",
    "@mdi/font": "^7.4.47",
    "@tonejs/midi": "^2.0.28",
    "async-wait-until": "^2.0.30",
    "eslint-plugin-vue": "^10.5.0",
    "iconv-lite": "^0.7.0",
    "lodash-es": "^4.17.21",
    "many-keys-map": "^3.0.3",
    "pinia": "^3.0.3",
    "playerioclient": "^1.0.7",
    "pngjs": "^7.0.0",
    "pw-js-api": "github:doomestee/pw-js-api#e61d93a11fe7c99c6e780a2d79b663eea8d498f4",
    "pw-js-world": "^0.4.3",
    "vue": "^3.5.22",
    "vue-router": "^4.5.1",
    "vuetify": "^3.10.4",
    "worker-timers": "^8.0.25"
  },
  "engines": {
    "node": ">=24.9.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^24.6.1",
    "@types/pngjs": "^6.0.5",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/eslint-config-typescript": "^14.6.0",
    "eslint": "^9.36.0",
    "eslint-config-prettier": "^10.1.8",
    "globals": "^16.4.0",
    "lint-staged": "^16.2.3",
    "prettier": "^3.6.2",
    "simple-git-hooks": "^2.13.1",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.45.0",
    "vite": "^7.1.7",
    "vite-plugin-checker": "^0.11.0",
    "vite-plugin-eslint2": "^5.0.4",
    "vite-plugin-node-polyfills": "^0.24.0",
    "vite-tsconfig-paths": "^5.1.4",
    "vue-tsc": "^3.1.0"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged && npm run ts-check"
  },
  "lint-staged": {
    "*": [
      "prettier --write --ignore-unknown",
      "eslint --fix"
    ]
  }
}
